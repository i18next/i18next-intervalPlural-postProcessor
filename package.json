{
  "name": "i18next-intervalplural-postprocessor",
  "version": "0.1.0",
  "description": "postProcessor that allows interval based plurals",
  "main": "./lib/index.js",
  "keywords": [
    "i18next",
    "i18next-postProcessor"
  ],
  "homepage": "https://github.com/i18next/i18next-intervalPlural-postProcessor",
  "bugs": "https://github.com/i18next/i18next-intervalPlural-postProcessor/issues",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.4"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "5.0.0-beta6",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "coveralls": "2.11.6",
    "eslint": "1.10.3",
    "gulp": "3.9.0",
    "gulp-bump": "1.0.0",
    "gulp-filter": "3.0.1",
    "gulp-git": "1.7.0",
    "gulp-prompt": "0.1.2",
    "gulp-shell": "0.5.2",
    "gulp-tag-version": "1.3.0",
    "gulp-uglify": "1.5.1",
    "i18next": "2.1.0",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "uglifyify": "3.0.1",
    "yargs": "3.32.0"
  },
  "scripts": {
    "coverage": "babel-node node_modules/.bin/isparta cover _mocha",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "transpile": "babel src --out-dir lib",
    "browserify": "browserify ./src/index.js --outfile ./bin/index.js -g uglifyify -t babelify -s i18nextIntervalPluralPostProcessor",
    "copy": "cp ./bin/index.js ./i18nextIntervalPluralPostProcessor.min.js",
    "tdd": "mocha test/*.js --compilers js:babel-register --watch",
    "test": "mocha test/*.js --compilers js:babel-register",
    "build": "npm run transpile && npm run browserify && npm run copy",
    "publish": "gulp publish",
    "patch": "npm run publish -- -v patch",
    "minor": "npm run publish -- -v minor",
    "major": "npm run publish -- -v major"
  },
  "author": "Jan MÃ¼hlemann <jan.muehlemann@gmail.com> (https://github.com/jamuhl)",
  "license": "MIT"
}
